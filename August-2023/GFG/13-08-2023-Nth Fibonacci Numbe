

//User function Template for javascript

/**
 * @param {number} n
 * @returns {number}
*/

class Solution {
    
    nthFibonacci(n)
    {
        let a = 1, b =1;
        
        for(let i=2; i<=n; i++) {
            b = (a+b);
            a = (b-a)% 1000000007
        }
        
        return a;
    }
}
