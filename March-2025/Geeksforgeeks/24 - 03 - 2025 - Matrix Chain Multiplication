class Solution:
    def matrixMultiplication(self, arr):
        n = len(arr)
        dp = [[0] * n for _ in range(n)]
        
        for l in range (2, n) :
            for i in range(1, (n - l + 1)) :
                j = i + l - 1
                dp[i][j] = float('inf')
                for k in range (i , j) :
                    cost = dp[i][k] + dp[k+1][j] + arr[i-1] * arr[k] * arr[j]
                    dp[i][j] = min(dp[i][j] , cost)
        
        return dp[1][n-1] if n > 1 else 0
